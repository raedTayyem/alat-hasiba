{
  "name": "alathasiba",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "analyze:translations": "node analyze-translations.cjs",
    "analyze:bundle": "vite build --mode analyze",
    "optimize:images": "node scripts/optimize-images.cjs",
    "optimize:svg": "node scripts/optimize-svg.cjs",
    "optimize:all": "npm run optimize:images && npm run optimize:svg",
    "ci": "npm run type-check && npm run lint && npm run test:run && npm run build",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\""
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@tailwindcss/typography": "^0.5.19",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^16.1.4",
    "react-router-dom": "^6.16.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@squoosh/cli": "^0.7.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.31",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.3.3",
    "terser": "^5.44.0",
    "typescript": "^5.0.2",
    "vite": "^7.1.12",
    "vitest": "^1.0.4"
  }
}
